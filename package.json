{
  "name": "a11y-demo-app",
  "version": "0.0.0",
  "private": true,
  "description": "Ember Accessibility Demo App",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "demo": "git checkout gh-pages && git branch --set-upstream-to=origin/gh-pages && git reset --hard origin/gh-pages && git pull && git checkout master && ember github-pages:commit --message \"Publish demo application.\" && git push origin gh-pages:gh-pages",
    "deploy": "npm run build && gh-pages -d dist"
  },
